{
  "name": "@ehacke/pubsub",
  "description": "Simple pubsub wrapper",
  "version": "1.0.0",
  "author": "Eric Hacke",
  "bugs": {
    "url": "https://github.com/ehacke/pubsub/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/bluebird": "^3.5.28",
    "@types/lodash": "^4.14.144",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.12",
    "bunyan-prettystream": "^0.1.3",
    "lodash": "^4.17.15",
    "ts-essentials": "^6.0.2",
    "tsconfig-paths": "^3.9.0"
  },
  "devDependencies": {
    "@ehacke/eslint-config": "^1.0.0",
    "@ehacke/prettier-config": "^1.0.1",
    "@google-cloud/pubsub": "^1.5.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai-as-promised": "^7.1.2",
    "@types/expect": "^24.3.0",
    "@types/luxon": "^1.15.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.1.8",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.0",
    "fixpack": "^3.0.6",
    "getenv": "^1.0.0",
    "husky": "^4.0.10",
    "lint-staged": "^10.0.1",
    "luxon": "^1.22.0",
    "mocha": "^7.0.0",
    "nyc": "^15.0.0",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "ts-node": "^8.6.2",
    "ts-sinon": "^1.0.24",
    "typedoc": "^0.16.9",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.7.5",
    "typings": "^2.1.1",
    "uuid": "^7.0.1"
  },
  "files": [
    "dist/**/*.{js,ts,tsbuildinfo}",
    "docs/**/*.*"
  ],
  "homepage": "https://github.com/ehacke/pubsub#readme",
  "keywords": [
    "gcp",
    "google",
    "pubsub",
    "wrapper"
  ],
  "license": "MIT",
  "lint-staged": {
    "package.json": [
      "fixpack"
    ],
    "**/*.{js,ts}": [
      "prettier-eslint --write",
      "eslint --fix",
      "npm run docs"
    ]
  },
  "main": "dist/index.js",
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "src/scratch.*"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary",
      "lcov"
    ]
  },
  "peerDependencies": {
    "@google-cloud/pubsub": "^1.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ehacke/pubsub.git"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.build.json && sed -i 's/\"files\":/\"_files\":/' dist/package.json",
    "commit": "git-cz",
    "docs": "typedoc --plugin typedoc-plugin-markdown --readme none --exclude \"tests/**/*.+(unit|it).+(ts|js)\"",
    "integration": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.it.+(ts|js)\"",
    "lint": "fixpack && prettier-eslint --write && eslint --fix \"**/*.{js,ts}\"",
    "major": "npm version major && npm publish",
    "minor": "npm version minor && npm publish",
    "patch": "npm version patch && npm publish",
    "postpublish": "git push",
    "prepare": "fixpack",
    "prepublishOnly": "npm run build",
    "test": "nyc mocha --recursive -r source-map-support/register -r ts-node/register -r tsconfig-paths/register --use_strict --exit \"tests/**/*.+(unit|it).+(ts|js)\""
  },
  "types": "dist/index.d.ts"
}
